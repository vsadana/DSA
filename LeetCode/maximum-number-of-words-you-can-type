// https://leetcode.com/problems/maximum-number-of-words-you-can-type/
func canBeTypedWords(_ text: String, _ brokenLetters: String) -> Int{
    var charSp = text.components(separatedBy: " ")
    for i  in Array(brokenLetters) {
        for (index,j) in charSp.enumerated() {
            if j.contains(i){
                charSp[index] = ""
            }
        }
    }
    return charSp.filter({
        $0 != ""
    }).count
}
